{
  "name": "pdf-reader-app",
  "version": "1.0.0",
  "description": "A React-based PDF reader with annotation features",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "cross-env PORT=4003 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "prod": "npm run build && node server.js"
  },
  "dependencies": {
    "@react-pdf-viewer/core": "^3.8.0",
    "@react-pdf-viewer/default-layout": "^3.8.0",
    "@react-pdf-viewer/full-screen": "^3.12.0",
    "@react-pdf-viewer/page-navigation": "^3.12.0",
    "@react-pdf-viewer/rotate": "^3.12.0",
    "@react-pdf-viewer/search": "^3.12.0",
    "@react-pdf-viewer/toolbar": "^3.12.0",
    "@react-pdf-viewer/zoom": "^3.12.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.4",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "lucide-react": "^0.479.0",
    "multer": "^1.4.5-lts.1",
    "pdfjs-dist": "^3.4.120",
    "postcss": "^8.4.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "sqlite3": "^5.1.6",
    "tailwindcss": "^3.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000"
}
